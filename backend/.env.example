# Supabase
SUPABASE=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_ROLE_KEY=your-supabase-service-role-key

# CORS â€” Set to your Vercel frontend URL after deploying
FRONTEND_URL=https://your-app.vercel.app

# Server port (Render sets this automatically; 8000 for local)
PORT=8000

# Differential Privacy
DP_ENABLED=false
DP_CLIP_NORM=500.0
DP_NOISE_MULT=0.01

# Federated Learning Settings
DATASET_NAME=
DATASET_ROOT=./data
NUM_CLIENTS=5
LOCAL_EPOCHS=1
BATCH_SIZE=32
LEARNING_RATE=0.01
OPTIMIZER=sgd
MIN_AGGREGATE_SIZE=1
MODEL_HIDDEN_DIMS=256,128
DEVICE=cpu
TEST_SPLIT_RATIO=0.2
NORM_THRESHOLD=1000.0
DISTANCE_THRESHOLD=500.0
TRIM_RATIO=0.1
BG_TRAIN_EPOCHS=3
